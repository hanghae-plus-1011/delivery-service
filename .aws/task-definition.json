{
    "family": "delivery-service-dev-task",
    "containerDefinitions": [
        {
            "name": "delivery-service",
            "image": "528402323646.dkr.ecr.ap-northeast-2.amazonaws.com/delivery-service-repository",
            "cpu": 0,
            "portMappings": [
                {
                    "name": "delivery-service-8000-tcp",
                    "containerPort": 8000,
                    "hostPort": 8000,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "environmentFiles": [],
            "mountPoints": [],
            "volumesFrom": [],
            "workingDirectory": "/app",
            "ulimits": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "delivery-service-dev-logs",
                    "awslogs-region": "ap-northeast-2",
                    "awslogs-stream-prefix": "ecs"
                }
            },
            "secrets": [
                {
                    "name": "AWS_ACCESS_KEY_ID",
                    "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:528402323646:secret:delivery-service/server-RmkdpM"
                },
                {
                    "name": "AWS_SECRET_ACCESS_KEY",
                    "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:528402323646:secret:delivery-service/server-RmkdpM"
                },
                {
                    "name": "AWS_REGION",
                    "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:528402323646:secret:delivery-service/server-RmkdpM"
                },
                {
                    "name": "AWS_LOG_GROUP_NAME",
                    "valueFrom": "arn:aws:secretsmanager:ap-northeast-2:528402323646:secret:delivery-service/server-RmkdpM"
                }
            ]
        }
    ],
    "taskRoleArn": "arn:aws:iam::528402323646:role/delivery-service-cicd-role",
    "executionRoleArn": "arn:aws:iam::528402323646:role/delivery-service-cicd-role",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "1024",
    "memory": "3072",
    "ephemeralStorage": {
        "sizeInGiB": 21
    },
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    }
}