{
    "family": "delivery-service-dev-task",
    "containerDefinitions": [
        {
            "name": "delivery-service",
            "image": "528402323646.dkr.ecr.ap-northeast-2.amazonaws.com/delivery-service-repository",
            "cpu": 0,
            "portMappings": [
                {
                    "name": "delivery-service-8000-tcp",
                    "containerPort": 8000,
                    "hostPort": 8000,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "LANGUAGE",
                    "value": "ko_KR.UTF-8"
                },
                {
                    "name": "NODE_ENV",
                    "value": "development"
                },
                {
                    "name": "LANG",
                    "value": "ko_KR.UTF-8"
                },
                {
                    "name": "TZ",
                    "value": "Asia/Seoul"
                }
            ],
            "environmentFiles": [],
            "mountPoints": [],
            "volumesFrom": [],
            "workingDirectory": "/app",
            "ulimits": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-create-group": "true",
                    "awslogs-group": "/ecs/delivery-service-dev-task",
                    "awslogs-region": "ap-northeast-2",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ],
    "taskRoleArn": "arn:aws:iam::528402323646:role/delivery-service-cicd-role",
    "executionRoleArn": "arn:aws:iam::528402323646:role/delivery-service-cicd-role",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "1024",
    "memory": "3072",
    "ephemeralStorage": {
        "sizeInGiB": 21
    },
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    }
}